<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.metain.web.mapper.AlarmMapper">
    <select id="alarmList" resultType="String">
        SELECT noti_content from TBL_NOTIFICATION
    </select>
    <insert id="insertAlarm" parameterType="AlarmDTO">
        INSERT INTO TBL_NOTIFICATION(EMP_ID,NOTI_CONTENT)
        VALUES(#{empId}, #{notiContent})
    </insert>
</mapper>